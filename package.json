{
  "name": "lang-parse",
  "version": "1.0.0",
  "description": "Parse input strings against a custom grammar, with semantic state extraction and suggestions for powering autocompletion and other interactive UXs",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "babel src -d dist"
  },
  "keywords": [
    "lang-parse",
    "grammar",
    "autocomplete",
    "prefix",
    "regex",
    "npm",
    "ESM",
    "commonJS",
    "typescript"
  ],
  "author": "Alex Heitzmann",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "strict": ["error", "global"]
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/indie-bloom/lang-parse.git"
  }
}